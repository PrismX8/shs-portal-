     <script>

(async () => {

  const url = "/sitemap_index.xml";

  console.log("Debug: Fetching sitemap to inspect headers...");

  try {

    const res = await fetch(url, { cache: "no-store" });

    console.log("Response status:", res.status);

    console.log("Response headers:");

    res.headers.forEach((val, key) => {

      console.log(`   ${key}: ${val}`);

    });

    const text = await res.text();

    console.log("Sitemap content:");

    console.log(text);

    const contentType = res.headers.get("content-type");

    if (!contentType) {

      console.warn("No content-type returned! Vercel may be ignoring vercel.json");

    } else if (!contentType.includes("application/xml")) {

      console.error("Wrong content-type! Should be application/xml");

    } else {

      console.log("Correct content-type (application/xml)");

    }

    if (res.redirected) {

      console.warn("The request was redirected, check your rewrites");

      console.log("Redirected to:", res.url);

    }

  } catch (err) {

    console.error("Debug: Failed to fetch sitemap:", err);

  }

})();

</script>



